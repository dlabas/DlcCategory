{
    "name": "dl-commons/dlc-category",
    "description": "A simple category module for Zend Framework 2 based applications.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "module",
        "zf2"
    ],
    "homepage": "https://github.com/dlabas/DlcCategory",
    "authors": [
        {
            "name": "Daniel Labas",
            "email": "daniel@dlabas.de",
            "homepage": "https://github.com/dlabas/"
        }
    ],
    "repositories": {
        "doctrine2-nestedset": {
            "type": "package",
            "package": {
                "name": "doctrine2-extensions/doctrine2-nestedset",
                "version": "dev-master",
                "source": {
                    "url": "git://github.com/blt04/doctrine2-nestedset.git",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        }
    },
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": ">2.1.3",
        "dl-commons/dlc-doctrine": "dev-master",
        "doctrine2-extensions/doctrine2-nestedset": "dev-master"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": {
            "DlcCategory": "src/",
            "DoctrineExtensions\\NestedSet": "vendor/doctrine2-extensions/doctrine2-nestedset/lib"
        }
    }
}
